
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Add Money</title>
    <link href="/Content/WithdrawStyle.css" rel="stylesheet" type="text/css" media="all">
</head>
<body>
    <div>
        <div class="title3">
            <h1 style="color:whitesmoke;">
                Add Chip
            </h1>
        </div>
        <div class="title2">
            <p style="color:whitesmoke;">
                Select the amount you wish to transform your money to chip
            </p>
            <p style="color:red;">Attention Here ! : 10 Chip = 1 TL, 10.000 Chip = 1000 TL</p>
            <p style="color:yellow">Your Chip :@Session["Chip"] Your Money : @Session["TL"]</p>
        </div>
        <div class="inputPadding">
            <input type="submit" value="  50 Chip   " name="Confirm">
            <input type="submit" value="  500 Chip  " name="Confirm">
            <input type="submit" value="  5000 Chip " name="Confirm">
        </div>
        <div class="inputPadding">
            <input type="submit" value="  100Chip   " name="Confirm">
            <input type="submit" value=" 1000 Chip " name="Confirm">
            <input type="submit" value="10.000 Chip" name="Confirm">
        </div>
        <div class="inputPadding">
            <input type="submit" value="  250 Chip  " name="Confirm">
            <input type="submit" value=" 2500 Chip " name="Confirm">
            <input type="submit" value=" Max Chip  " name="Confirm">
        </div>
    </div>
</body>
</html>
